#ifdef __OBJC__

#import <Foundation/Foundation.h>
#import <QuartzCore/QuartzCore.h>

#import <CocoaLumberjack/CocoaLumberjack.h>

static const DDLogLevel ddLogLevel = DDLogLevelVerbose;

#define LogError(fmt, ...)  DDLogError(fmt, ##__VA_ARGS__)
#define LogWarn(fmt, ...)   DDLogWarn(fmt, ##__VA_ARGS__)
#define LogInfo(fmt, ...)   DDLogInfo(fmt, ##__VA_ARGS__)
#define LogDebug(fmt, ...)  DDLogDebug(fmt, ##__VA_ARGS__)
#define LogTrace(fmt, ...)  DDLogVerbose(fmt, ##__VA_ARGS__)

#define TIME_START CFTimeInterval startTime = CACurrentMediaTime(); CFTimeInterval endTime;
#define TIME_RESTART startTime = CACurrentMediaTime();
#define TIME_END endTime = CACurrentMediaTime(); LogDebug(@"timed: %1.2f", endTime - startTime);

#define min(a, b) (b < a ? b : a)
#define max(a, b) (a < b ? b : a)

#endif
